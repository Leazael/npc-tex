{
  "commentChar": "^",
  "keywords": [
    {
      "keywords": ["name"],
      "latex": {
        "command": "name"
      }
    },
    {
      "keywords": ["wim"],
      "separators": ["|", ":", "|", "|"],
      "latex": {
        "command": "table"
      }
    }
  ],
  "environments": [
    {
      "environment": "Category",
      "chars": ["="],
      "ignoreChars": "=",
      "latex": {
        "command": "category",
        "nInputs": 1
      }
    },
    {
      "environment": "Daily Spells and Spell-like abilities.",
      "chars": ["$", ["[","("], ["]",")"], [":", "--"]],
      "includeInputs": [true, true, false, true],
      "latex": {
        "command": "dailyspells",
        "nInputs": 3
      }
    }
  ]
}
